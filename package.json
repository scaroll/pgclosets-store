{
  "name": "pg-closets-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "agents:report": "node scripts/agents/report-assets.js",
    "agents:escape": "node scripts/agents/escape-apostrophes.js",
    "agents:run": "node scripts/agents/run-agents.js",
    "agents:convert-anchors": "node scripts/agents/convert-anchors.js",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:images:test": "node scripts/optimize-images.js --test",
    "image:optimize": "node scripts/optimize-images-production.js",
    "image:validate": "node scripts/validate-image-optimization.js",
    "image:audit": "node scripts/image-audit-comprehensive.js",
    "image:clean": "node scripts/clean-unused-images.js",
    "division2:execute": "ts-node scripts/division2-renin-integration.ts",
    "division2:test": "ts-node scripts/division2-renin-integration.ts --limit 10",
    "division2:clean": "rm -rf data/renin public/products/renin",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix",
    "lint:design-system": "next lint --config .eslintrc.design-system.json",
    "lint:report": "next lint --format json --output-file reports/eslint-report.json",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:incremental": "tsc --noEmit --incremental",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "quality:fix": "npm run lint:fix && npm run format && npm run type-check",
    "quality:report": "node scripts/quality/generate-report.js",
    "validate:design": "./scripts/enforce-design-system.sh",
    "analyze": "ANALYZE=true npm run build",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "analyze:bundle": "ANALYZE=true next build",
    "analyze:deps": "node scripts/quality/analyze-dependencies.js",
    "analyze:complexity": "node scripts/quality/code-complexity.js",
    "perf": "npm run analyze-bundle && npm run build",
    "validate:performance": "node scripts/performance-validation.js",
    "perf:all": "npm run analyze-bundle && npm run validate:performance",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "node scripts/quality/security-check.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:types": "tsd",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check && npm run test",
    "env:generate": "node scripts/generate-secrets.js",
    "fix:all": "npm run lint:fix && npm run format && npm run quality:fix",
    "validate:all": "npm run quality && npm run security:audit && npm run test",
    "deploy:prod": "vercel --prod --yes",
    "alias:prod": "bash scripts/ops/release-prod.sh alias-only",
    "verify:prod": "node scripts/ops/verify-deploy.mjs",
    "release:prod": "bash scripts/ops/release-prod.sh",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete all",
    "email:dev": "email dev --dir emails --port 3001"
  },
